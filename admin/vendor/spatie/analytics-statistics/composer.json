{
    "name": "spatie/analytics-statistics",
    "description": "An opinionated php package to retrieve Google Analytics data.",
    "keywords": [
        "spatie",
        "google-analytics"
    ],
    "homepage": "https://github.com/spatie/analytics-statistics",
    "license": "MIT",
    "authors": [
    {
      "name": "Freek Van Der Herten",
      "email": "freek@spatie.be",
      "homepage": "https://murze.be",
      "role": "Developer"
    },
    {
      "name": "Matthias De Winter",
      "email": "dewinter.matthias@gmail.com",
      "homepage": "https://github.com/MatthiasDeWinter",
      "role": "Developer"
    },
    {
      "name": "Sebastian De Deyne",
      "email": "sebastiandedeyne@gmail.com",
      "homepage": "https://github.com/sebastiandedeyne",
      "role": "Developer"
    }
  ],
    "require": {
        "php" : ">=5.3.0",
        "google/apiclient" : "1.1.*",
        "nesbot/carbon": "^1.19"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Analytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Analytics\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
